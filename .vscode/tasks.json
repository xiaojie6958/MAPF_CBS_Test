{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Release",
      "type": "shell",
      "command": "catkin_make",
      "args": [
        "-DCMAKE_EXPORT_COMPILE_COMMANDS=Yes",
        "-DCMAKE_BUILD_TYPE=Release"
      ],
      "group": "build",
      "presentation": {
        "reveal": "always"
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": "Debug",
      "type": "shell",
      "command": "catkin_make",
      "args": [
        "-DCMAKE_EXPORT_COMPILE_COMMANDS=Yes",
        "-DCMAKE_BUILD_TYPE=Debug"
      ],
      "group": "build",
      "presentation": {
        "reveal": "always"
      },
      "problemMatcher": "$msCompile"
    },
  ]
}
